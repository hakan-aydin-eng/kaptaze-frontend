{
  "expo": {
    "name": "kapkazan",
    "slug": "kaptaze",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1",
            "useFrameworks": "static"
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#16a34a",
          "defaultChannel": "default"
        }
      ],
      "expo-location"
    ],
    "splash": {
      "image": "./assets/splash-kapkazan.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#16a34a"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kaptaze.app",
      "buildNumber": "113",
      "jsEngine": "hermes",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIRequiresFullScreen": true,
        "NSLocationWhenInUseUsageDescription": "Bu uygulama yakınındaki restoranları göstermek için konum bilginizi kullanır.",
        "NSLocationAlwaysUsageDescription": "Bu uygulama yakınındaki restoranları göstermek için konum bilginizi kullanır.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Bu uygulama yakınındaki restoranları göstermek için konum bilginizi kullanır.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "LSApplicationQueriesSchemes": [
          "googlemaps",
          "comgooglemaps"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD9Y6wbMftggdkvi7JSAbQYeqBVEpwhBz8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#16a34a"
      },
      "package": "com.kaptaze.app",
      "versionCode": 2,
      "jsEngine": "hermes",
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD9Y6wbMftggdkvi7JSAbQYeqBVEpwhBz8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "haknaydin",
    "updates": {
      "enabled": false
    },
    "extra": {
      "eas": {
        "projectId": "20e11315-3be4-4335-af55-44487c0e423a"
      }
    }
  }
}